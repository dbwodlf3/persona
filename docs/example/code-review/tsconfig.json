{
    "compilerOptions": {
        "resolveJsonModule": true,
        "target": "ES2020",
        "lib": [
            "ES2020",
        ],
        "module": "CommonJS",
        "inlineSourceMap": true,
        "rootDir": "./",
        "removeComments": true,
        "strict": true,
        "baseUrl": "./src",
        "outDir": "./dest",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "downlevelIteration": true
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": []
}